{
  "editor.tabSize": 2,
  "files.encoding": "utf8",
  "files.insertFinalNewline": true,
  "files.associations": {
    "*.md": "mdx"
  },
  "files.exclude": {
    ".git": true
  },
  "prettier.tabWidth": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "npm.packageManager": "yarn",
  "editor.detectIndentation": false
}
